{
  "name": "@hackycy/egg-bull",
  "version": "0.0.4",
  "description": "bull plugin for EggJs",
  "eggPlugin": {
    "name": "bull"
  },
  "egg": {
    "typescript": true,
    "declarations": true,
    "framework": "egg",
    "tsHelper": {
      "watchDirs": {
        "queue": {
          "enabled": true,
          "generator": "auto",
          "directory": "app/queue",
          "declareTo": "Application.queue",
          "caseStyle": "camel",
          "trigger": ["add", "unlink", "change"]
        }
      }
    }
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin"
  ],
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.16.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "egg-ts-helper": "^1.25.8",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "egg-bin pkgfiles --check && npm run lint && npm run cov",
    "pkgfiles": "egg-bin pkgfiles",
    "autod": "autod"
  },
  "files": [
    "app.js",
    "lib/**/*.js",
    "package.json",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "ci": {
    "version": "8, 10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hackycy/egg-bull.git"
  },
  "bugs": {
    "url": "https://github.com/hackycy/egg-bull/issues"
  },
  "homepage": "https://github.com/hackycy/egg-bull#readme",
  "author": "hackycy",
  "license": "MIT"
}
